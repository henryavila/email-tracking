			count: 1
			path: src/DataObjects/Mailgun/DeliveryStatus.php

		-
			message: '#^Strict comparison using \=\=\= between ''HenryAvila\\\\EmailTracking\\\\Events\\\\Email\\\\AbstractEmailEvent'' and ''failed'' will always evaluate to false\.$#'
			identifier: identical.alwaysFalse
			count: 1
			path: src/Events/Email/AbstractEmailEvent.php

		-
			message: '#^Using nullsafe property access on non\-nullable type HenryAvila\\EmailTracking\\DataObjects\\Mailgun\\DeliveryStatus\. Use \-\> instead\.$#'
			identifier: nullsafe.neverNull
			count: 1
			path: src/Events/Email/TemporaryFailureEmailEvent.php

		-
