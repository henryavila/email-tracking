			identifier: property.phpDocType
			count: 1
			path: src/Mail/TrackableMail.php

		-
			message: '#^Method HenryAvila\\EmailTracking\\Middleware\\Webhooks\\MailgunWebhookMiddleware\:\:buildSignature\(\) is unused\.$#'
			identifier: method.unused
			count: 1
			path: src/Middleware/Webhooks/MailgunWebhookMiddleware.php

