{
    "name": "apps-inteligentes/email-tracking",
    "description": "Track e-mail delivery with Mailgun Hooks and display the report on Laravel Nova Dashboard",
    "version": "1.0.2",
    "keywords": [
        "apps-inteligentes",
        "laravel",
        "email-tracking"
    ],
    "homepage": "https://github.com/apps-inteligentes/email-tracking",
    "license": "MIT",
    "authors": [
        {
            "name": "Henry √Åvila"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-permission": "^3.0|^5.0"
    },
    "require-dev": {
        "laravel/nova": "^3.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AppsInteligentes\\EmailTracking\\": "src",
            "AppsInteligentes\\EmailTracking\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppsInteligentes\\EmailTracking\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AppsInteligentes\\EmailTracking\\EmailTrackingServiceProvider"
            ],
            "aliases": {
                "EmailTracking": "AppsInteligentes\\EmailTracking\\Facades\\EmailTracking"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
